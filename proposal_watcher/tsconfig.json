{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "types": ["node", "bun-types", "inquirer"]
  }
}
